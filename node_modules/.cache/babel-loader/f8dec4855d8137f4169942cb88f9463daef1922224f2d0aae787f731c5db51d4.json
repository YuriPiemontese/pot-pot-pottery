a class=\"product-price\">{{ item.price }}</a\n      >\\\n    </div>\n  </div>\n</template>\n\n<script>\nimport SingleOverlay from \"./SingleOverlay.vue\";\nimport productList from \"../assets/data/product-list.json\";\n\nexport default {\n  name: \"MyProduct\",\n\n  components: {\n    SingleOverlay,\n  },\n  data() {\n    return {\n      products: [],\n      showSingleOverlay: false,\n    };\n  },\n\n  mounted() {\n    const parsedData = JSON.parse(productList);\n    this.products = parsedData.products;\n  },\n\n  methods: {\n    openOverlay() {\n      this.showSingleOverlay = !this.showSingleOverlay;\n      this.$emit(\"open-overlay\");\n    },\n    closeOverlay() {\n      this.showSingleOverlay = !this.showSingleOverlay;\n      this.$emit(\"close-overlay\");\n    },\n  },\n};\n</script>\n\n<style scoped>\na {\n  font-size: 36px;\n  line-height: 36px;\n  letter-spacing: -0.03em;\n  font-weight: 500;\n}\n\n.product-wrapper {\n  margin-top: 10px;\n  margin-left: 10px;\n  margin-right: 10px;\n  margin-bottom: 10px;\n  width: 453px;\n  height: 634px;\n  background-color: #c4c4c4;\n  position: relative;\n  display: inline-block;\n}\n\n.product-info {\n  background-color: none;\n  min-height: 72px;\n  position: absolute;\n  bottom: 0;\n  margin-bottom: 15px;\n  margin-left: 18px;\n  vertical-align: baseline;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}